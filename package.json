{
  "name": "replane-sdk",
  "version": "0.0.3",
  "description": "Replane core TypeScript SDK.",
  "type": "module",
  "license": "MIT",
  "homepage": "https://github.com/tilyupo/replane-javascript#readme",
  "bugs": {
    "url": "https://github.com/tilyupo/replane-javascript/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tilyupo/replane-javascript.git"
  },
  "author": "tilyupo <npm@tilyupo.dev>",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "replane",
    "config",
    "configuration",
    "sdk",
    "feature-flags"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "release": "bumpp && npm publish"
  },
  "devDependencies": {
    "@types/node": "^22.15.17",
    "bumpp": "^10.1.0",
    "tsdown": "^0.11.9",
    "typescript": "^5.8.3",
    "vitest": "^3.1.3"
  },
  "packageManager": "pnpm@10.7.0+sha1.66453f13fbf9078d3db193718206a8d738afdbdb"
}
