{
  "name": "@replanejs/svelte",
  "version": "0.8.12",
  "description": "Svelte SDK for Replane - feature flags and remote configuration",
  "type": "module",
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "prebuild": "node ../../scripts/update-version.js .",
    "build": "svelte-package -i src -o dist",
    "dev": "svelte-package -i src -o dist --watch",
    "typecheck": "svelte-check --tsconfig ./tsconfig.json",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepublishOnly": "pnpm run build",
    "release": "pnpm run build && bumpp && npm publish"
  },
  "keywords": [
    "replane",
    "svelte",
    "feature-flags",
    "remote-config",
    "configuration",
    "sdk"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/replane-dev/replane-javascript",
    "directory": "packages/svelte"
  },
  "bugs": {
    "url": "https://github.com/replane-dev/replane-javascript/issues"
  },
  "homepage": "https://github.com/replane-dev/replane-javascript#readme",
  "peerDependencies": {
    "svelte": ">=4.0.0"
  },
  "dependencies": {
    "@replanejs/sdk": "workspace:^"
  },
  "devDependencies": {
    "@sveltejs/package": "^2.3.10",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/svelte": "^5.2.6",
    "@types/node": "^22.19.3",
    "bumpp": "^10.1.0",
    "jsdom": "^27.3.0",
    "svelte": "^5.0.0",
    "svelte-check": "^4.1.4",
    "typescript": "^5.4.0",
    "vite": "^6.0.0",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
